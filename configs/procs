#
#  <Process ID>|<Event handler>|<Message ID suffix 2 letters>
#
echo	|EchoHandler		|ec|DIR.IN=/usr/local/games/exch/proc/echo/in/;DIR.OUT=
mailout	|MailOutHandler		|mo|DIR.IN=/usr/local/games/exch/proc/mailout/in/;DIR.OUT=
mailin	|MailInHandler		|mi|DIR.IN=/usr/local/games/exch/proc/mailin/in/;DIR.OUT=
switch	|SwitchHandler		|sw|DIR.IN=/usr/local/games/exch/data/ftp/cortex/from/;DIR.OUT=
eraser	|EraserHandler		|er|DIR.IN=/usr/local/games/exch/proc/eraser/in/;DIR.OUT=
filein	|FileInHandler		|fi|DIR.IN=/usr/local/games/exch/proc/filein/in/;DIR.OUT=
fileout	|FileOutHandler		|fo|DIR.IN=/usr/local/games/exch/proc/fileout/in/;DIR.OUT=
csin	|CSInHandler		|ci|DIR.IN=/usr/local/games/exch/data/samba/in/;DIR.OUT=
csout	|CSOutHandler		|co|DIR.IN=/usr/local/games/exch/proc/csout/in/;DIR.OUT=/usr/local/games/exch/data/samba/out/
